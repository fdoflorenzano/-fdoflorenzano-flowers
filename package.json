{
  "name": "@fdoflorenzano/flowers",
  "version": "0.0.1",
  "description": "Some util functions for axidraw prints",
  "keywords": [
    "axidraw",
    "ts",
    "utils",
    "mechanic",
    "design"
  ],
  "homepage": "https://github.com/fdoflorenzano/-fdoflorenzano-flowers",
  "bugs": {
    "url": "https://github.com/fdoflorenzano/-fdoflorenzano-flowers/issues"
  },
  "author": {
    "name": "Fernando Florenzano",
    "email": "f.florenzano94@gmail.com",
    "url": "https://fdoflorenzano.cl/"
  },
  "license": "ISC",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --ext .ts",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "bumpp": "^9.2.0",
    "eslint": "^8.51.0",
    "esno": "^0.17.0",
    "lint-staged": "^14.0.1",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  }
}
